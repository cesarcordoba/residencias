md-card
	.templates


md-card
	.cotizador
		md-card-content(layout="row")
			md-icon search
			md-autocomplete(
			flex
			md-selected-item="ctrl.selectedCliente" 
			md-search-text-change="ctrl.BuscarClienteChange(ctrl.searchTextCliente)" 
			md-search-text="ctrl.searchTextCliente" 
			md-selected-item-change="ctrl.ClienteSeleccionadoChange(item)" 
			md-items="item in clientes" 
			md-item-text="item.nombre" 
			md-min-length="0" 
			placeholder="Buscar..." 
			md-menu-class="autocomplete-custom-template")
				md-item-template
					span {{item.nombre}}
			md-button(md-colors="{background: 'yellow'}")
				md-icon view_headline
			md-button(md-colors="{background: 'yellow'}")
					md-icon replay

