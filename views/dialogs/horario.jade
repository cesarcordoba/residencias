md-dialog
	md-toolbar
		.md-toolbar-tools
			h4 Nuevo Horario
			span(flex)
			md-button(ng-click="close()")
				md-icon clear
	form.md-padding(layout="row" ng-submit="submit(horario)")
		md-input-container(flex)
			label Horario
			input(ng-model="horario.horario" required)
		md-button.md-raised(type="submit")
			md-icon save

